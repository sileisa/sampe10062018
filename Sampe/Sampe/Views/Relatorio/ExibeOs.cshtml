@model IEnumerable<Sampe.FormularioOrdemServico>

@{
	ViewBag.Title = "ExibeOs";
	Layout = "~/Views/Relatorio/LayoutRelatorio.cshtml";
}
@foreach (var item in Model)
{
<div>
	<h4>FormularioOrdemServico</h4>
	<hr />
	<dl class="dl-horizontal">
		<dt>
			@Html.DisplayNameFor(model => model.Maquina.NomeMaquina)
		</dt>
		<dd>
			@Html.DisplayFor(model => item.Maquina.NomeMaquina)
		</dd>
		<dt>
			@Html.DisplayNameFor(model => model.Usuario.NomeUsuario)
		</dt>
		<dd>
			@Html.DisplayFor(model => item.Usuario.NomeUsuario)
		</dd>
		<dt>
			@Html.DisplayNameFor(model => model.TipoManutencao)
		</dt>
		<dd>
			@Html.DisplayFor(model => item.TipoManutencao)
		</dd>
		<dt>
			@Html.DisplayNameFor(model => model.HoraInicio)
		</dt>
		<dd>
			@Html.DisplayFor(model => item.HoraInicio)
		</dd>
		<dt>
			@Html.DisplayNameFor(model => model.HoraFinal)
		</dt>
		<dd>
			@Html.DisplayFor(model => item.HoraFinal)
		</dd>
		<dt>
			@Html.DisplayNameFor(model => model.Dt)
		</dt>
		<dd>
			@Html.DisplayFor(model => item.Dt)
		</dd>
		<dt>
			@Html.DisplayNameFor(model => model.Intervalo)
		</dt>
		<dd>
			@Html.DisplayFor(model => item.Intervalo)
		</dd>
		<dt>
			@Html.DisplayNameFor(model => model.IntervaloInicio)
		</dt>
		<dd>
			@Html.DisplayFor(model => item.IntervaloInicio)
		</dd>
		<dt>
			@Html.DisplayNameFor(model => model.IntervaloFim)
		</dt>
		<dd>
			@Html.DisplayFor(model => item.IntervaloFim)
		</dd>
		<dt>
			@Html.DisplayNameFor(model => model.ObsIntervalo)
		</dt>
		<dd>
			@Html.DisplayFor(model => item.ObsIntervalo)
		</dd>
		<dt>
			@Html.DisplayNameFor(model => model.Supervisor)
		</dt>
		<dd>
			@Html.DisplayFor(model => item.Supervisor)
		</dd>
		<dt>
			@Html.DisplayNameFor(model => model.FormularioOSAtividades)
		</dt>
		<dd>
			@foreach (var x in item.FormularioOSAtividades)
			{
				<label>@Html.DisplayFor(modelItem => x.StatusOS) @x.AtividadeOS.NomeAtvOs</label><br />
			}
		</dd>

	</dl>
	<label>@Html.DisplayFor(model => item.Status) Validação do Supervisor</label>
</div>
}